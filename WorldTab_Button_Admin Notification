local k={}

for l,m in pairs(g)do
for n,o in ipairs(m)do
j[o]=l
end
end

for l,m in ipairs(f)do
k[m]=l
end

local l=Instance.new"ScreenGui"
l.Name="AdminNotifier"
l.ResetOnSpawn=false
l.ZIndexBehavior=Enum.ZIndexBehavior.Sibling
l.Parent=e

