


local a=game:GetService"Players"
local b=game:GetService"RunService"

local c=a.LocalPlayer




local d=0.01
local e=2

local f=7000
local g=800
local h=Vector3.new(0,0,0)




local i=0
local j=0
local k

local l,m,n=0,0,0




local function getHRP(o)
return o and o:FindFirstChild"HumanoidRootPart"
end

local function getRandomTargetHRP()
local o={}

for p,q in ipairs(a:GetPlayers())do
if q~=c then
local r=q.Character
local s=r and r:FindFirstChildOfClass"Humanoid"
local t=getHRP(r)

if s and t and s.Health>0 then
table.insert(o,t)
end
end
end

if#o==0 then
return nil
end

return o[math.random(1,#o)]
end




b.Heartbeat:Connect(function(o)
i+=o
j+=o


if not k or j>=e then
k=getRandomTargetHRP()
j=0
end

if i<d then return end
i=0

local p=c.Character
local q=getHRP(p)
if not q then return end
if not k then return end


if not k.Parent then
k=nil
return
end


local r=math.rad(f)
l+=r*o
m+=r*o
n+=r*o
local s=CFrame.Angles(l,m,n)


local t=k.CFrame*CFrame.new(h)
q.CFrame=t*s


q.AssemblyLinearVelocity=k.CFrame.LookVector*g
end)
