local z=false
local A

local function stopFlash()
z=false
if A then
A:Disconnect()
A=nil
end

c:Create(q,TweenInfo.new(0.4),{
Transparency=1
}):Play()

c:Create(s,TweenInfo.new(0.4),{
BackgroundColor3=Color3.fromRGB(100,100,110)
}):Play()
end

local function startFlash()
if z then return end
z=true

c:Create(s,TweenInfo.new(0.3),{
BackgroundColor3=Color3.fromRGB(255,80,80)
}):Play()

local B=0
A=b.RenderStepped:Connect(function(C)
B=B+C*6
local D=(math.sin(B)+1)/2
local E=60+(195*D)
q.Color=Color3.fromRGB(255,E,E)
q.Transparency=0.2+(0.6*D)
end)
end

local function createAdminRow(B,C,D)
local E=k[C]or 10
local F=i[E]or Color3.fromRGB(255,255,255)

local G=Instance.new"Frame"
G.Name="AdminRow"
G.Size=UDim2.new(1,0,0,22)
G.BackgroundColor3=Color3.fromRGB(30,30,38)
G.BackgroundTransparency=0.6
G.BorderSizePixel=0
G.LayoutOrder=D

local H=Instance.new"UICorner"
H.CornerRadius=UDim.new(0,6)
H.Parent=G

local I=Instance.new"UIStroke"
I.Color=F
I.Transparency=0.75
I.Thickness=1
I.Parent=G

local J=Instance.new"Frame"
J.Position=UDim2.new(0,6,0.5,0)
J.AnchorPoint=Vector2.new(0,0.5)
J.Size=UDim2.new(0,48,0,16)
J.BackgroundColor3=F
J.BackgroundTransparency=0.88
J.BorderSizePixel=0
J.Parent=G

local K=Instance.new"UICorner"
K.CornerRadius=UDim.new(0,4)
K.Parent=J

local L=Instance.new"UIStroke"
L.Color=F
L.Transparency=0.4
L.Thickness=1
L.Parent=J

local M=Instance.new"TextLabel"
M.Size=UDim2.new(1,0,1,0)
M.BackgroundTransparency=1
M.Font=Enum.Font.GothamBold
M.Text=h[C]or"???"
M.TextColor3=F
M.TextSize=9
M.Parent=J

local N=Instance.new"TextLabel"
N.Position=UDim2.new(0,60,0,0)
N.Size=UDim2.new(1,-66,1,0)
N.BackgroundTransparency=1
N.Font=Enum.Font.GothamMedium
N.Text="@"..B.Name
N.TextColor3=F
N.TextSize=11
N.TextXAlignment=Enum.TextXAlignment.Left
N.TextTruncate=Enum.TextTruncate.AtEnd
N.Parent=G

G.BackgroundTransparency=1
c:Create(G,TweenInfo.new(0.25,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{
BackgroundTransparency=0.6
}):Play()

return G
end

local function updateAdminList()
for B,C in ipairs(x:GetChildren())do
if not C:IsA"UIListLayout"then
C:Destroy()
end
end

local B={}
for C,D in ipairs(a:GetPlayers())do
local E=j[D.UserId]
if E then
table.insert(B,{
player=D,
rank=E,
index=k[E]or 999
})
end
end

table.sort(B,function(C,D)
return C.index<D.index
end)

local C=#B
v.Text=C==0 and"No admins detected"or
(C==1 and"1 admin online"or C.." admins online")

if C==0 then
c:Create(m,TweenInfo.new(0.35,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{
Size=UDim2.new(0,280,0,50)
}):Play()
stopFlash()
else
for D,E in ipairs(B)do
local F=createAdminRow(E.player,E.rank,D)
F.Parent=x
end

local D=52+(C*22)+((C-1)*5)+8
c:Create(m,TweenInfo.new(0.35,Enum.EasingStyle.Quad,Enum.EasingDirection.Out),{
Size=UDim2.new(0,280,0,D)
}):Play()

startFlash()
end
end

updateAdminList()

a.PlayerAdded:Connect(function()
task.wait(0.1)
updateAdminList()
end)

a.PlayerRemoving:Connect(updateAdminList)

task.spawn(function()
while l.Parent do
task.wait(2)
updateAdminList()
end
end)
